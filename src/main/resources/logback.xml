<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%t] %d{HH:mm:ss.SSS} %-5p %c{1} - %m%n</pattern>
		</encoder>
	</appender>

	<logger name="taskdurations" level="INFO">
		<appender-ref ref="taskdurations" />
	</logger>

	<root level="INFO">
		<appender-ref ref="file" />
		<appender-ref ref="stdout" />
	</root>
</configuration>
